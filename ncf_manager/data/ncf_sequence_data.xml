<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for ncf.sequence -->
        <record id="seq_ncf_registry" model="ir.sequence">
            <field name="name">NCF Norma 07-2018</field>
            <field name="prefix">B</field>
            <field name="padding">8</field>
            <field name="use_date_range" eval="False"/>
            <field name="ncf_control" eval="True"/>
