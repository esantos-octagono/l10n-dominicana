        <record id="ir_sequence_date_range_id_final" model="ir.sequence.date_range">
            <field name="sequence_id" ref="seq_ncf_registry"/>
            <field name="date_to" eval="(DateTime.today()).strftime('%Y-%m-%d %H:%M')"/>
            <field name="date_from" eval="(DateTime.today()).strftime('%Y-%m-%d %H:%M')"/>
            <field name="number_next">1</field>
            <field name="max_number_next">1</field>
            <field name="sale_fiscal_type">final</field>
